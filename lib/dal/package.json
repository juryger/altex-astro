{
  "name": "dal",
  "type": "module",
  "version": "0.0.1",
  "exports": "./src/index.ts",
  "main": "./src/index.ts",
  "scripts": {
    "generate:general": "drizzle-kit generate --name=general --config=drizzle-general.config.ts",
    "generate:operations": "drizzle-kit generate --name=operations --config=drizzle-operations.config.ts",
    "generate:catalog": "drizzle-kit generate --name=catalog --config=drizzle-catalog.config.ts",
    "generate": "npm run generate:operations && npm run generate:general && npm run generate:catalog",
    "migrate:general": "drizzle-kit migrate --config=drizzle-general.config.ts",
    "migrate:operations": "drizzle-kit migrate --config=drizzle-operations.config.ts",
    "migrate:catalog": "drizzle-kit migrate --config=drizzle-catalog.config.ts",
    "migrate": "npm run migrate:operations && npm run migrate:general && npm run migrate:catalog",
    "replica": "cp /Users/iuriig/Sources/altex-astro/db/catalog.db /Users/iuriig/Sources/altex-astro/db/replicas/catalog-initial.db",
    "build": "rm -rf dist && tsc -b .",
    "preview": "node --env-file=.env.development ./dist/index.js"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.0",
    "drizzle-orm": "^0.45.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.5",
    "drizzle-kit": "^0.31.8"
  },
  "description": "Data Access Layer (DAL) package based on Sqlite and Drizzle ORM",
  "keywords": [
    "sqlite",
    "drizzle",
    "orm",
    "nodejs"
  ],
  "author": "Iurii Gerasimov",
  "license": "ISC"
}
